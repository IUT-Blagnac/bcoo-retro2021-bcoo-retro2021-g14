'---------------------------
'Messages et contenus
'----------------------------
class Message
{
idMessage
contenu
destinataire
messageLu

envoyer()
supprimer()
répondre()

}

Media <|-- Message
class Media 
{
fichier
tailleFichier
}



'----------------------------




'----------------------------
'Les lisisons
'------------------------------
Serveur "1" -- "*"  Role 
Serveur "1" -- "*..50"  Emoji 
Serveur "1" -- "*..50"  Salon 
Serveur "*..100" -- "1..*"  Compte : est membre
Serveur "1" -- "*"  Compte : est propriétaire
Groupe "*" -- "1..10" Compte : est membre
Groupe "*" -- "1" Compte : est propriétaire
Compte "1" -- "*" Message : envoi





