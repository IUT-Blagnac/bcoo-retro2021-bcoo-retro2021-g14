'------------------------------
'Classes diverses
'------------------------------

class Groupe
{
idGroupe
nomGroupe
imageGroupe

}

<> diamondAppel

class Appel 
{
    dateDebut
    dateFin 
    tpsTotal 
    appeler()
    répondre()
